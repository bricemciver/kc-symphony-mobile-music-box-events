{
  "name": "kc-symphony-mobile-music-box-events",
  "version": "0.0.1",
  "private": true,
  "description": "Read events from the KC Symphony Mobile Music Box webpage and format into JSON for programatic use.",
  "author": "Brice McIver (github@bricemciver.com)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/aws-lambda": "^8.10.159",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(/test/.*|\\.(spec))\\.(ts)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
